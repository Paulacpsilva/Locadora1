[32m+[m	[32mprivate List<String> filmes = new ArrayList<>();[m
[32m+[m[41m	[m
[32m+[m	[32mpublic List<String> getFilmes() throws FileNotFoundException {[m
[32m+[m		[32mScanner scan = new Scanner(new File("filmes.txt"));[m
[32m+[m		[32mwhile(scan.hasNextLine()) {[m
[32m+[m			[32mString linha = scan.nextLine();[m
[32m+[m			[32mfilmes.add(linha);[m
[32m+[m		[32m}[m
[32m+[m			[32mreturn filmes;[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mpublic void removerFilme(String nome) {[m
[32m+[m		[32mfilmes.remove(nome);[m
